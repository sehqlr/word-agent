# This loads in NLTK when the python shells inits

print("Running commands")

try:
		import nltk
except ImportError:
		print("Module NLTK unavailable")
else:
		print("NLTK imported successfully")

try:
		from nltk.book import *
except ImportError:
		print("NLTK data unavailable")
else:
		print("NLTK data loaded successfully")

print("Finished RC")
